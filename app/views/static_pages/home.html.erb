<div class="center jumbotron">
  <h1><%= full_title %></h1>
  <p>
    <%= t('community_page_description') %>
  </p>
  <!-- news anchor -->
  <div id="news"></div>
</div>
<div class="row">
  <div class="col-md-12">
    <%= render 'shared/feed' %>
  </div>

  <% if logged_in? %>
      <% if current_user.admin? %>
          <div class="col-md-12 new-post">
            <%= render 'shared/post_form' %>
          </div>
      <% end %>
  <% end %>
</div>